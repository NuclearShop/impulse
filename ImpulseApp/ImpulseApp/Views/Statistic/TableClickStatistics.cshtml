@model IEnumerable<ImpulseApp.Models.StatModels.TableRowModel>

<table class="table table-striped table-bordered table-hover" id="ClickStat">
    <thead>
        <tr>
            <td>
                @Html.LabelFor(a => a.First().IP)
            </td>
            <td>
                @Html.LabelFor(a => a.First().Locale)
            </td>
            <td>
                @Html.LabelFor(a => a.First().Browser)
            </td>
            @foreach (var st in Model.First().Stages)
            {
                <td>@Html.DisplayFor(model => st.Name)</td>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(model => m.IP)
                </td>
                <td>
                    @Html.DisplayFor(model => m.Locale)
                </td>
                <td>
                    @Html.DisplayFor(model => m.Browser)
                </td>
                @foreach (var st in m.Stages)
                {
                    <td>@Html.DisplayFor(model => st.Value)</td>
                }
            </tr>
        }
    </tbody>
</table>