@model IEnumerable<ImpulseApp.Models.StatModels.TableRowModel>

<table class="table table-striped table-bordered table-hover" id="ClickStat">
    <thead>
        <tr>
            <td>
                @Html.LabelFor(a => a.First().IP)
            </td>
            <td>
                @Html.LabelFor(a => a.First().Locale)
            </td>
            <td>
                @Html.LabelFor(a => a.First().Browser)
            </td>
            <td>
                @Html.LabelFor(a => a.First().FirstStage)
            </td>
            <td>
                @Html.LabelFor(a => a.First().SecondStage)
            </td>
            <td>
                @Html.LabelFor(a => a.First().ThirdStage)
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(model => m.IP)
                </td>
                <td>
                    @Html.DisplayFor(model => m.Locale)
                </td>
                <td>
                    @Html.DisplayFor(model => m.Browser)
                </td>
                <td>
                    @Html.DisplayFor(model => m.FirstStage)
                </td>
                <td>
                    @Html.DisplayFor(model => m.SecondStage)
                </td>
                <td>
                    @Html.DisplayFor(model => m.ThirdStage)
                </td>
            </tr>
        }
    </tbody>
</table>